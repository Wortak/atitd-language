translate DarkestNight to german masculine
{
swa "Dunkelste Nacht";
  sn "Dunkelste Nacht"; sg "Dunkelste Nacht"; sd "Dunkelste Nacht"; sa "Dunkelste Nacht";
  pn "Dunkelste Nacht"; pg "Dunkelste Nacht"; pd "Dunkelste Nacht"; pa "Dunkelste Nacht";
